@model EducationCms.Web.Areas.admin.Models.Pages.PageModel

<h1 class="mt-2 mb-5">Add Page</h1>
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample" method="post" asp-controller="Page" asp-action="Create">
                    <div class="row">


                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Name">Name</label>
                                <input type="text" class="form-control" asp-for="Name" placeholder="Name">
                            </div>
                        </div>


                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Title">Title</label>
                                <input type="text" class="form-control" asp-for="Title" placeholder="Title">
                            </div>
                        </div>



                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Description">Description</label>
                                <textarea type="text" class="form-control tinyMce" asp-for="Description" placeholder="Description"></textarea>
                            </div>
                        </div>


                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Title2">Title second</label>
                                <input type="text" class="form-control" asp-for="Title2" placeholder="Title second">
                            </div>
                        </div>



                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Description2">Description second</label>
                                <textarea type="text" class="form-control tinyMce" asp-for="Description2" placeholder="Description second"></textarea>
                            </div>
                        </div>

                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Title3">Title third</label>
                                <input type="text" class="form-control" asp-for="Title3" placeholder="Title third">
                            </div>
                        </div>



                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Description3">Description third</label>
                                <textarea type="text" class="form-control tinyMce" asp-for="Description3" placeholder="Description third"></textarea>
                            </div>
                        </div>

                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Title4">Title four</label>
                                <input type="text" class="form-control" asp-for="Title4" placeholder="Title third">
                            </div>
                        </div>



                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Description4">Description four</label>
                                <textarea type="text" class="form-control tinyMce" asp-for="Description4" placeholder="Description third"></textarea>
                            </div>
                        </div>


                        <div class="col-sm-12">
                            <div class="form-group">
                                <label asp-for="Icon">Icon</label>
                                <select class="form-control" asp-for="Icon">
                                    
                                    <option value="icon-tick"><span class="icon-tick"></span>                     </option>
                                    <option value="icon-magnifying-glass1"><span class="icon-magnifying-glass1"></span>        </option>
                                    <option value="icon-right-arrow1"><span class="icon-right-arrow1"></span>             </option>
                                    <option value="icon-wealth"><span class="icon-wealth"></span>                   </option>
                                    <option value="icon-data-analytics"><span class="icon-data-analytics"></span>           </option>
                                    <option value="icon-report"><span class="icon-report"></span>                   </option>
                                    <option value="icon-phone-ringing"><span class="icon-phone-ringing"></span>            </option>
                                    <option value="icon-mobile-analytics"><span class="icon-mobile-analytics"></span>         </option>
                                    <option value="icon-research"><span class="icon-research"></span>                 </option>
                                    <option value="icon-creative"><span class="icon-creative"></span>                 </option>
                                    <option value="icon-video"><span class="icon-video"></span>                    </option>
                                    <option value="icon-help"><span class="icon-help"></span>                     </option>
                                    <option value="icon-customer-review"></span<span class="icon-customer-review"></span>          </option>
                                    <option value="icon-consultant">    <span class="icon-consultant"></span>               </option>
                                    <option value="icon-checkmark">   <span class="icon-checkmark"></span>                </option>
                                    <option value="icon-bank">        <span class="icon-bank"></span>                     </option>
                                    <option value="icon-protection">    <span class="icon-protection"></span>               </option>
                                    <option value="icon-travel">        <span class="icon-travel"></span>                   </option>
                                    <option value="icon-software">      <span class="icon-software"></span>                 </option>
                                    <option value="icon-consulting">    <span class="icon-consulting"></span>               </option>
                                    <option value="icon-group">         <span class="icon-group"></span>                    </option>
                                    <option value="icon-consulting-1">  <span class="icon-consulting-1"></span>             </option>
                                    <option value="icon-creative-1">    <span class="icon-creative-1"></span>               </option>
                                    <option value="icon-analysis">      <span class="icon-analysis"></span>                 </option>
                                    <option value="icon-business">      <span class="icon-business"></span>                 </option>
                                    <option value="icon-global">        <span class="icon-global"></span>                   </option>
                                    <option value="icon-team1">         <span class="icon-team1"></span>                    </option>
                                    <option value="icon-magnifying-glass"><span class="icon-magnifying-glass"></span>         </option>
                                    <option value="icon-shopping-cart"> <span class="icon-shopping-cart"></span>            </option>
                                    <option value="icon-right-arrow">   <span class="icon-right-arrow"></span>              </option>
                                    <option value="icon-furnitures">    <span class="icon-furnitures"></span>               </option>
                                    <option value="icon-buildings">     <span class="icon-buildings"></span>                </option>
                                    <option value="icon-house-design">  <span class="icon-house-design"></span>             </option>
                                    <option value="icon-architect">    <span class="icon-architect"></span>                </option>
                                    <option value="icon-architect-1">   <span class="icon-architect-1"></span>              </option>
                                    <option value="icon-architect-2">   <span class="icon-architect-2"></span>              </option>
                                    <option value="icon-measure">       <span class="icon-measure"></span>                  </option>
                                    <option value="icon-blueprint">     <span class="icon-blueprint"></span>                </option>
                                    <option value="icon-kitchen">       <span class="icon-kitchen"></span>                  </option>
                                    <option value="icon-architect-3">   <span class="icon-architect-3"></span>              </option>
                                    <option value="icon-architect-4">  <span class="icon-architect-4"></span>              </option>
                                    <option value="icon-architect-5">   <span class="icon-architect-5"></span>              </option>
                                    <option value="icon-satisfaction">  <span class="icon-satisfaction"></span>             </option>
                                    <option value="icon-team">          <span class="icon-team"></span>                     </option>
                                    <option value="icon-checklist">     <span class="icon-checklist"></span>                </option>
                                    <option value="icon-outsourcing">   <span class="icon-outsourcing"></span>              </option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <button type="submit" class="btn btn-success mr-2">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>



