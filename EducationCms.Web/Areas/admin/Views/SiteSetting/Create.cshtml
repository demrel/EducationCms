@model EducationCms.Web.Areas.admin.Models.SIteSettings.SiteSettingsModel

<h1 class="mt-2 mb-5">Site Settings</h1>
<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <form class="forms-sample" method="post" asp-controller="SiteSetting" asp-action="Create" enctype="multipart/form-data">
                    <div class="row">

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="Facebook">Facebook</label>
                                <input type="text" class="form-control" asp-for="Facebook" placeholder="Facebook">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="Youtube">Youtube</label>
                                <input type="text" class="form-control" asp-for="Youtube" placeholder="Youtube">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="Instagram">Youtube</label>
                                <input type="text" class="form-control" asp-for="Instagram" placeholder="Instagram">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="Twitter">Youtube</label>
                                <input type="text" class="form-control" asp-for="Twitter" placeholder="Instagram">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="BuisnesStartTime">Buisness Start Date</label>
                                <input type="date" class="form-control" asp-for="BuisnesStartTime">
                            </div>
                        </div>


                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="PhoneNumber">Phone Number</label>
                                <input type="text" class="form-control" asp-for="PhoneNumber" placeholder="Phone Number">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="Email">Email</label>
                                <input type="text" class="form-control" asp-for="Email" placeholder="Email">
                            </div>
                        </div>

                        <div class="col-sm-4">
                            <div class="form-group">
                                <label asp-for="ContactTitle">Contact Title</label>
                                <input type="text" class="form-control" asp-for="ContactTitle" placeholder="Contact Title">
                            </div>
                        </div>

                    </div>
                    <button type="submit" class="btn btn-success mr-2">Submit</button>
                </form>
            </div>
        </div>
    </div>
</div>




@section Scripts{
    <script src="~/admin/js/shared/file-upload.js"></script>
}
<!-- main-panel ends -->
